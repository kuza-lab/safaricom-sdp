{\xe \v newrelic_end_transaction\:newrelic.php}
{\xe \v newrelic.php\:newrelic_end_transaction}
\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid {
